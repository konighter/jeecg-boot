<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hanzhan.tech.workbench.candidate.mapper.CandidateResumeMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  hz_candidate_resume 
		WHERE
			 candidate_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="com.hanzhan.tech.workbench.candidate.entity.CandidateResume">
		SELECT * 
		FROM  hz_candidate_resume
		WHERE
			 candidate_id = #{mainId} 	</select>
</mapper>
